#header > .contents_con > .list_con > ul > li > a {font-size:18px; color:#222; font-weight:600; position:relative;}
#header > .contents_con > .list_con > ul > li > a:after {content:''; width:0; height:2px; background:#0095D5; position:absolute; bottom:-4px; left:0; right:0; transition:all 0.3s ease;}
